{
  "name": "flickit",
  "version": "1.0.0",
  "description": "Flick and read educational app",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jdamore/flickit.git"
  },
  "author": "Jean R. D'Amore",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jdamore/flickit/issues"
  },
  "homepage": "https://github.com/jdamore/flickit#readme",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --reporter nyan --compilers js:babel-core/register,css:test/nocss-compiler.js --require test/dom.js --require babel-polyfill test/**/*-test.js",
    "watch": "webpack --watch",
    "build": "webpack -p --progress --colors --config webpack.prod.config.js",
    "dev": "webpack-dev-server --progress --colors --port 8090",
    "start": "npm run watch | npm run dev",
    "feature": "node_modules/.bin/mocha feature"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "testDirectoryName": "test",
    "scriptPreprocessor": "<rootDir>/jest-preprocessor.js",
    "setupEnvScriptFile": "<rootDir>/jest-environment.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/src/infra/utils"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "config": "^1.17.1",
    "karma-cli": "^0.1.2",
    "react": "^0.14.4",
    "react-dom": "^0.14.4",
    "react-soundplayer": "^0.3.1",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "babel-core": "^6.1.2",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "chai": "^3.4.1",
    "chai-spies": "^0.7.1",
    "css-loader": "^0.22.0",
    "file-loader": "^0.8.4",
    "http-server": "^0.8.5",
    "jasmine-core": "^2.4.1",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.1",
    "pact-consumer-js-dsl": "^0.2.1",
    "proxyquire": "^1.7.3",
    "randomstring": "^1.1.4",
    "react-addons-test-utils": "^0.14.4",
    "react-tools": "^0.13.3",
    "sass-loader": "^3.1.1",
    "selenium-webdriver": "^2.48.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
