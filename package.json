{
  "name": "flickit",
  "version": "1.0.0",
  "description": "Flick and read educational app",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jdamore/flickit.git"
  },
  "author": "Jean R. D'Amore",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jdamore/flickit/issues"
  },
  "homepage": "https://github.com/jdamore/flickit#readme",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "build": "webpack --progress --colors",
    "dev": "npm run build | webpack-dev-server --progress --colors --port 8090"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "testDirectoryName": "test",
    "scriptPreprocessor": "<rootDir>/jest-preprocessor.js",
    "setupEnvScriptFile": "<rootDir>/jest-environment.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "devDependencies": {
    "babel-core": "^6.1.2",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "css-loader": "^0.22.0",
    "file-loader": "^0.8.4",
    "http-server": "^0.8.5",
    "jest-cli": "^0.8.1",
    "jest-webpack-alias": "^2.0.0",
    "node-sass": "^3.4.1",
    "react-addons-test-utils": "~0.14.3",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
